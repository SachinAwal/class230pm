   ],
   "source": [
    "s = []\n",
    "a = [[\"Ram\",700],[\"Hari\",900],[\"Sita\",1500]]\n",
    "for i in range(len(a)):\n",
    "    s.append(a[i][1])\n",
    "\n",
    "b = max(s)\n",
    "t = s.index(b)\n",
    "print(a[t])"
